    <Color x:Key="Gray900">#333</Color>
    <Color x:Key="Gray950">#222</Color>
    <Color x:Key="Gray999">#111</Color>
    <Color x:Key="PSText">#999</Color>
    <Color x:Key="Shadow">Black</Color>

    <!--flyout-->
    <Color x:Key="FlyoutHeaderRunningText">#f5a</Color>
    <Color x:Key="FlyoutBackground">WhiteSmoke</Color>
    <Color x:Key="FlyoutHeaderPart1">Blue</Color>
    <Color x:Key="FlyoutHeaderPart2">Yellow</Color>
    <Color x:Key="FlyoutHeaderPart3">BlueViolet</Color>
    <Color x:Key="FlyoutHeaderPart4">Green</Color>
    <Color x:Key="FlyoutHeaderBackground">#222</Color>
    <Color x:Key="FlyoutMenuItemSeparator">#ddd</Color>
    <Color x:Key="FlyoutFooter">Green</Color>

    <!--tabbar-->
    <Color x:Key="TabBarBackground">#111</Color>
    <SolidColorBrush x:Key="TabBarMainButtonShadow" Color="Blue"/>

    <!--displayAlertPage-->
    <Color x:Key="DisplayAlertBackground">#555</Color>
    <Color x:Key="DisplayAlertText">White</Color>
    <Color x:Key="DisplayAlertSeparator">#777</Color>
    <Color x:Key="DisplayAlertButton">#fb58aa</Color>

    <!--base-->
    <Color x:Key="Switcher">Green</Color>
    <Color x:Key="SwitcherOnSurface">Yellow</Color>
    <Color x:Key="SwitcherOffSurface">#eee</Color>
    <Color x:Key="PickerTitle">Blue</Color>
    <Color x:Key="Button">#777</Color>
