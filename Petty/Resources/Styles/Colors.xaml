    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#512BD4</Color>
    <Color x:Key="Secondary">#DFD8F7</Color>
    <Color x:Key="Tertiary">#2B0B98</Color>
    <Color x:Key="Gray000">#fff</Color>
    <Color x:Key="Gray100">#eee</Color>
    <Color x:Key="Gray200">#ddd</Color>
    <Color x:Key="Gray300">#ccc</Color>
    <Color x:Key="Gray400">#bbb</Color>
    <Color x:Key="Gray500">#aaa</Color>
    <Color x:Key="Gray600">#999</Color>
